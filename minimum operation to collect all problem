class solution{
public int minoperation(list<integer> nums. int k){
int count=0;
arraylist<integer> list1= new arraylist<>();
for(int i=1; i<=k; i++){
list1.add(i};
}

for(int j= nums.size()-1; j>=0; j--){
if(list1.isEmpty())
{
break;
}

if(list1.contains(nums.get(j)))
{
list1.remove(nums.get(j)):
}

count++
}

return count;
}
}
